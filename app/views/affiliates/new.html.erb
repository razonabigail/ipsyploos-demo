<h1>New Affiliate Registration</h1>

<%= form_for @affiliate do |f| %>
	<%= f.label :"user id" %> <br>
	<%= f.text_field :user_id %> <br>
	<br>
	<%= f.label :"last name" %> <br>
	<%= f.text_field :lastname %> <br>
	<br>
	<%= f.label :"first name" %> <br>
	<%= f.text_field :firstname %> <br>
	<br>
	<%= f.label :email %> <br>
	<%= f.text_field :email %> <br>
	<br>
	<%= f.submit %>
<% end %>